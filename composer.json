{
    "name": "sonata-project/block-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataBlockBundle",
    "keywords": [
        "sonata",
        "block"
    ],
    "homepage": "https://sonata-project.org/bundles/block",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataBlockBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/common": "^2.3",
        "psr/cache": "^1.0",
        "sonata-project/cache": "^1.0 || ^2.0",
        "sonata-project/core-bundle": "^3.15.1",
        "sonata-project/doctrine-extensions": "^1.1",
        "symfony/asset": "^4.3 || ^5.0",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^3.2 || ^4.3",
        "symfony/event-dispatcher": "^4.3",
        "symfony/form": "^3.2 || ^4.3",
        "symfony/framework-bundle": "^3.2 || ^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/http-kernel": "^3.2 || ^4.3",
        "symfony/options-resolver": "^4.3",
        "symfony/templating": "^4.3 || ^5.0",
        "symfony/twig-bundle": "^3.2 || ^4.3",
        "twig/twig": "^1.34 || ^2.0"
    },
    "conflict": {
        "jms/di-extra-bundle": "<1.7.0"
    },
    "require-dev": {
        "jms/di-extra-bundle": "^1.7",
        "knplabs/knp-menu-bundle": "^2.0 || ^3.0",
        "phpspec/prophecy": "^1.10",
        "sonata-project/admin-bundle": "^3.22",
        "symfony/debug": "^4.3 || ^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/stopwatch": "^5.0"
    },
    "suggest": {
        "jms/di-extra-bundle": "Annotations for Block definition",
        "knplabs/knp-menu-bundle": "^2.0 || ^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\BlockBundle\\": "src/"
        },
        "files": [
            "src/Resources/stubs/symfony2.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\BlockBundle\\Tests\\": "tests/"
        }
    }
}
